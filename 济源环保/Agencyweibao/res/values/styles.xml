<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="@android:style/Theme.Light.NoTitleBar.Fullscreen">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->

    </style>

    <style name="WeatherModule">
        <item name="android:background">@drawable/module_bg</item>
        <item name="android:padding">@dimen/padding_sm</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="MyCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_selector</item>
    </style>

    <!-- PopupWindows -->
    <style name="grouplist_item_textview">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <!-- PopupWindows -->

    <style name="pressure_dragview_text">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>

    <style name="Dotted">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1.0dip</item>
        <item name="android:layout_marginBottom">0.0dip</item>
        <item name="android:src">@drawable/dotted_line_624</item>
        <item name="android:scaleType">fitXY</item>
    </style>

    <style name="roomRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/room_rating_bar</item>
        <item name="android:minHeight">30dp</item>
        <item name="android:maxHeight">30dp</item>
    </style>

    <style name="roomRatingBar2" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/room_rating_bar2</item>
        <item name="android:minHeight">16dp</item>
        <item name="android:maxHeight">16dp</item>
    </style>

    <!-- 设置透明Theme -->
    <style name="Theme.AppStartLoadTranslucent" parent="android:Theme">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="pressure_dragview_text_dark">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">#b3ffffff</item>
    </style>

    <style name="main_tab_bottom">
        <item name="android:textColor">#ffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">0.0dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:button">@null</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">10sp</item>
    </style>

    <style name="MainTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowShowWallpaper">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="base_listview_style" parent="@android:style/Widget.ListView">
        <item name="android:scrollbarThumbVertical">@drawable/base_list_scrollbar_handle</item>
        <item name="android:fadingEdgeLength">0.0dip</item>
        <item name="android:alwaysDrawnWithCache">true</item>
        <item name="android:listSelector">@drawable/base_list_selector</item>
        <item name="android:drawSelectorOnTop">false</item>
        <item name="android:cacheColorHint">@null</item>
        <item name="android:divider">@null</item>
        <item name="android:dividerHeight">0.0dip</item>
    </style>

    <style name="base_small_progressbar_style">
        <item name="android:maxWidth">18.0dip</item>
        <item name="android:maxHeight">18.0dip</item>
        <item name="android:indeterminateDrawable">@anim/base_loading_small_anim</item>
        <item name="android:minWidth">18.0dip</item>
        <item name="android:minHeight">18.0dip</item>
    </style>

    <style name="base_large_progressbar_style">
        <item name="android:maxWidth">38.0dip</item>
        <item name="android:maxHeight">38.0dip</item>
        <item name="android:indeterminateDrawable">@anim/base_loading_large_anim</item>
        <item name="android:minWidth">38.0dip</item>
        <item name="android:minHeight">38.0dip</item>
    </style>

    <style name="aqi_option_style">
        <item name="android:textSize">11dp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@drawable/aqi_option_bg</item>
        <item name="android:paddingTop">7dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginTop">2.5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_marginBottom">2.5dp</item>
    </style>

    <style name="title_update_progressbar_style">
        <item name="android:maxWidth">45.0dip</item>
        <item name="android:maxHeight">45.0dip</item>
        <item name="android:indeterminateDrawable">@anim/title_update_anim</item>
        <item name="android:minWidth">45.0dip</item>
        <item name="android:minHeight">45.0dip</item>
    </style>

    <style name="ActionBarProgressBar" parent="@style/title_update_progressbar_style"></style>

    <style name="life_text1">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">#B663A4</item>
    </style>

    <style name="life_text2">
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#345946</item>
    </style>

    <style name="map_toolboxview">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_margin">10dp</item>
    </style>

    <style name="clockWeatherText">
        <item name="android:singleLine">true</item>
        <item name="android:shadowColor">#aa000000</item>
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">3.0</item>
        <item name="android:shadowRadius">2.5</item>
    </style>

    <style name="chat_text_date_style">
        <item name="android:padding">2dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">12sp</item>
        <item name="android:background">#bfbfbf</item>
    </style>

    <style name="chat_content_date_style">
        <item name="android:minHeight">50dp</item>
        <item name="android:gravity">left|center</item>
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">15sp</item>
        <item name="android:lineSpacingExtra">2dp</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:background">#bfbfbf</item>
    </style>

    <style name="chat_text_name_style">
        <item name="android:textColor">#818181</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="startTheme" parent="@android:style/Theme">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
    </style>
    <!-- 公众号删除添加关注dialog样式 -->

    <style name="MyDialogStyle" parent="@android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/pubdialoa</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>

    <!-- 地图长按弹出窗口的style -->
    <style name="mapLongclickPopupAnimation" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/map_longclick_panel_enter</item>
        <item name="android:windowExitAnimation">@anim/map_longclick_panel_exit</item>
    </style>

    <style name="Scrubber">
        <item name="android:padding">@dimen/padding_sm</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/negative_margin_sm</item>
        <item name="android:layout_marginRight">@dimen/negative_margin_sm</item>
        <item name="android:src">@drawable/grip</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <style name="load_dialog" parent="@android:style/Theme.DeviceDefault.Dialog">

        <!-- 设置完全透明 -->
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 设置无边框 -->
        <!-- <item name="android:windowFrame">@null</item> -->
        <!-- 设置无标题 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 设置为浮动窗口 -->
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <!-- 设置动画 -->
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <!-- 设置屏幕变暗 -->
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="spinner_style">
        <item name="android:background">@drawable/spinner_selector</item>
        <item name="android:paddingLeft">5dip</item>
    </style>

    <style name="stylebefore">
        <item name="android:textSize">13sp</item>
        <!-- <item name="android:textColor">@color/setbefore</item> -->
    </style>

    <style name="citytownavgstylebefore">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/citytownavgsetbefore</item>
    </style>

    <style name="stylenormal">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="my_edittext_style" parent="@android:style/Widget.EditText">
        <item name="android:background">@drawable/selector_et_rounded</item>
    </style>
</resources>