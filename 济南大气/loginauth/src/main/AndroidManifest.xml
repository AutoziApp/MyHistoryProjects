<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.com.mapuni.loginauth">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">

        <activity
            android:name="cn.com.mapuni.loginauth.meshingtotal.LoadingActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="cn.com.mapuni.loginauth.meshingtotal.LoginActivity" />
        <activity android:name="cn.com.mapuni.loginauth.meshingtotal.update.UpdateApkActivity" />

        <service android:name="cn.com.mapuni.loginauth.meshingtotal.update.DownloadService" />

    </application>

</manifest>
