    <string name="create_V_YDZF_RWXX">CREATE VIEW [V_YDZF_RWXX]
AS
SELECT
* FROM T_YDZF_RWXX left  join T_WRY_QYJBXX on T_YDZF_RWXX.QYDM=T_WRY_QYJBXX.QYDM</string>
    <string name="create_V_YDZF_RWXX_B">CREATE VIEW [V_WRY_ZXJK_GasOutputPollutant]
AS
select distinct b.* from [T_WRY_ZXJK_GasOutput] a left  join [T_WRY_ZXJK_OutputPollutant] b on 
a.OutportCode=b.OutportCode and a.entcode=b.entcode left join T_WRY_ZXJK_Pollutant
 c on b.PollutantCode = c.PollutantCode</string>
    <string name="create_V_WRY_ZXJK_WaterOutputPollutant">CREATE VIEW [V_WRY_ZXJK_WaterOutputPollutant]
AS
select distinct b.* from [T_WRY_ZXJK_WaterOutput] a left  join [T_WRY_ZXJK_OutputPollutant] b on 
a.OutportCode=b.OutportCode and a.entcode=b.entcode left join T_WRY_ZXJK_Pollutant
 c on b.PollutantCode = c.PollutantCode</string>
