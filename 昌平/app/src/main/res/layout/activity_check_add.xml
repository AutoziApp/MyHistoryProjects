<?xml version="1.0" encoding="utf-8"?>


    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        tools:context="com.mapuni.mobileenvironment.activity.ExecuteAddActivity"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">
    <include layout="@layout/toolbar_activity"
        android:id="@+id/include"></include>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/include"
            android:id="@+id/scrollView2" >
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="60dp"
                    android:layout_marginTop="10dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="30dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/check_the_team"
                        android:textSize="15sp"
                        android:textColor="@color/input_text_color"
                        android:layout_marginRight="10dp"/>

                    <com.mapuni.mobileenvironment.widget.NiceSpinner
                        android:id="@+id/spinnerA"
                        android:layout_width="200dp"
                        android:layout_height="40dp"
                        android:drawableRight="@mipmap/select"
                        />
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="60dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="30dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/check_tel"
                        android:textSize="15sp"
                        android:textColor="@color/input_text_color"
                        android:layout_marginRight="10dp"/>

                    <EditText
                        android:layout_width="200dp"
                        android:layout_height="40dp"
                        android:background="@drawable/kuang"
                        android:textSize="14sp"
                        android:textColor="@color/gray"
                        android:paddingLeft="5dp"
                        android:singleLine="true"/>

                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="60dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="30dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/check_source"
                        android:textSize="15sp"
                        android:textColor="@color/input_text_color"
                        android:layout_marginRight="10dp"/>

                    <com.mapuni.mobileenvironment.widget.NiceSpinner
                        android:id="@+id/spinnerB"
                        android:layout_width="200dp"
                        android:layout_height="40dp"
                        android:drawableRight="@mipmap/select"
                        />
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="60dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="30dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/check_adress"
                        android:textSize="15sp"
                        android:textColor="@color/input_text_color"
                        android:layout_marginRight="10dp"/>

                    <EditText
                        android:layout_width="200dp"
                        android:layout_height="40dp"
                        android:background="@drawable/kuang"
                        android:textSize="14sp"
                        android:textColor="@color/gray"
                        android:paddingLeft="5dp"
                        android:singleLine="true"/>
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="60dp"
                    android:layout_marginTop="10dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="30dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/check_is_execute"
                        android:textSize="15sp"
                        android:textColor="@color/input_text_color"
                        android:layout_marginRight="10dp"/>


                    <com.mapuni.mobileenvironment.widget.NiceSpinner
                        android:id="@+id/spinnerC"
                        android:layout_width="200dp"
                        android:layout_height="40dp"
                        android:drawableRight="@mipmap/select"
                        />
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="60dp"
                    android:layout_marginTop="10dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="30dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/check_censor"
                        android:textSize="15sp"
                        android:textColor="@color/input_text_color"
                        android:layout_marginRight="10dp"/>

                    <com.mapuni.mobileenvironment.widget.NiceSpinner
                        android:id="@+id/spinnerD"
                        android:layout_width="200dp"
                        android:layout_height="40dp"
                        android:drawableRight="@mipmap/select"
                        />
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="60dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="30dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/check_start_time"
                        android:textSize="15sp"
                        android:textColor="@color/input_text_color"
                        android:layout_marginRight="10dp"/>


                    <TextView
                        android:id="@+id/timeView"
                        android:layout_width="200dp"
                        android:layout_height="40dp"
                        android:background="@drawable/selector"
                        android:textSize="14sp"
                        android:textColor="@color/gray"
                        android:paddingLeft="5dp"
                        android:drawableRight="@drawable/iconfont_rili"
                        android:paddingRight="10dp"
                        android:singleLine="true"/>
                </LinearLayout>

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="80dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="30dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/check_type"
                        android:textSize="15sp"
                        android:textColor="@color/input_text_color"
                        android:layout_marginRight="10dp"/>
                    <com.mapuni.mobileenvironment.widget.NiceSpinner
                        android:id="@+id/spinnerE"
                        android:layout_width="200dp"
                        android:layout_height="40dp"
                        android:drawableRight="@mipmap/select"
                        />
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="80dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="30dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/check_marker"
                        android:textSize="15sp"
                        android:textColor="@color/input_text_color"
                        android:layout_marginRight="10dp"/>

                    <EditText
                        android:layout_width="200dp"
                        android:layout_height="60dp"
                        android:background="@drawable/kuang"
                        android:textSize="14sp"
                        android:textColor="@color/gray"
                        android:paddingLeft="5dp"
                        />
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="150dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="30dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/check_info"
                        android:textSize="15sp"
                        android:textColor="@color/input_text_color"
                        android:layout_marginRight="10dp"/>

                    <EditText
                        android:layout_width="200dp"
                        android:layout_height="130dp"
                        android:background="@drawable/kuang"
                        android:textSize="14sp"
                        android:textColor="@color/gray"
                        android:paddingLeft="5dp"
                        />
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    android:layout_gravity="right"
                    android:layout_marginRight="30dp"
                    android:gravity="center_vertical">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/check_remark"
                        android:textSize="15sp"
                        android:textColor="@color/input_text_color"
                        android:layout_marginRight="10dp"/>

                    <EditText
                        android:layout_width="200dp"
                        android:layout_height="80dp"
                        android:background="@drawable/kuang"
                        android:textSize="14sp"
                        android:textColor="@color/gray"
                        android:paddingLeft="5dp"
                        />
                </LinearLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="60dp"
                    android:layout_gravity="center_horizontal"
                    android:gravity="center_vertical"
                   >

                    <Button
                        android:layout_width="wrap_content"
                        android:layout_height="35dp"
                        android:layout_marginRight="50dp"
                        android:background="@drawable/cancel_bg"
                        android:text="@string/cancel"
                        android:textColor="@color/cancel_text"
                        android:id="@+id/cancel" />

                    <Button
                        android:layout_width="wrap_content"
                        android:layout_height="35dp"
                        android:text="@string/commit"
                        android:background="@drawable/commit_bg"
                        android:textColor="@color/white"
                        android:id="@+id/commit" />
                </LinearLayout>
            </LinearLayout>
        </ScrollView>


</RelativeLayout>
